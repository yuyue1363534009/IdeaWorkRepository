            function workPlaceToggle() {
                var deviceWidth = $(window).width() // 设备屏幕宽度
                var isMobel = deviceWidth <= 768 ? true : false // 判断是否为移动端
                var addressItem = $('.address-box .address-tab .tab-ul li')

                addressItem.unbind('click')
                addressItem.prevLi = 0
                if (isMobel) { // 移动端的地址选择切换
                    $('.address-box .address-tab .tab-ul-box .mc-left').hide();
                    $('.address-box .address-tab .tab-ul-box .mc-left').hide();
                    addressItem.removeClass('active-li').on('click', function (e) {
                        var currentLi = $(e.currentTarget)
                        var isSameLi = true
                        if (addressItem.prevLi && addressItem.prevLi[0] !== currentLi[0]) { // 收起上一个li
                            liTriggle(addressItem.prevLi)
                            isSameLi = false
                        }
                        liTriggle(currentLi)
                        if (addressItem.prevLi == 0) {
                            addressItem.prevLi = currentLi
                        } else {
                            if (isSameLi) {
                                addressItem.prevLi = 0
                            } else {
                                addressItem.prevLi = currentLi
                            }
                        }

                        function liTriggle(currentLi) {
                            currentLi.find('.box').slideToggle(function () {
                                if ($(this).is(":visible")) {
                                    currentLi.find('.label').addClass('active')
                                } else {
                                    currentLi.find('.label').removeClass('active')
                                }
                            });
                        }
                    })
                } else { // PC端的情况
                    addressItem.on('click', function () {
                        addressItem.removeClass('active-li');
                        $(this).addClass('active-li');
                    })

                    $('.address-box .address-tab .tab-ul').css('right', 'auto');
                    $('.address-box .address-tab .tab-ul').css('left', 0);
                    $('.address-box .address-tab .tab-ul-box .mc-right').show();
                    $('.address-box .address-tab .tab-ul-box .mc-left').hide();
                    $('.address-box .address-tab .tab-ul .li-4').addClass('active-li')
                }
            }

